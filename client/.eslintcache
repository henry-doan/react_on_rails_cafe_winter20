ents/menus/MenuForm.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/menus/Menu.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/items/Items.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/menus/MenuList.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/providers/MenuProvider.js",["75"],"import React, { Component } from 'react';\nimport axios from 'axios';\nimport { Redirect } from 'react-router-dom';\n\nconst MenuContext = React.createContext();\n\nexport const MenuConsumer = MenuContext.Consumer;\n\nclass MenuProvider extends Component {\n  state = { menus: [] }\n\n  componentDidMount() {\n    // grab our menus from db\n    axios.get('/api/menus')\n      .then( res => {\n        // set to state \n        this.setState({ menus: res.data })\n      })\n      .catch( err => {\n        console.log(err);\n      })\n  }\n\n  addMenu = (menu) => {\n    // add to the db\n    axios.post('/api/menus', { menu })\n    .then(res => {\n        // add to the state\n        const { menus } = this.state; \n        this.setState({ menus: [...menus, res.data]})\n      })\n      .catch( err => {\n        console.log(err);\n      })\n  }\n\n  updateMenu = (id, menu) => {\n    // update to the db\n    axios.put(`/api/menus/${id}`, { menu })\n      .then( res => {\n        // update to the state\n        const menus = this.state.menus.map( m => {\n          if (m.id === id) {\n            return res.data\n          } \n          return m\n        })\n        this.setState({ menus })\n      })\n      .catch( err => {\n        console.log(err);\n      })\n  }\n\n  deleteMenu = (id) => {\n    // delete in the db\n    axios.delete(`/api/menus/${id}`)\n      .then( res => {\n        // delete in the state\n        const { menus } = this.state\n        this.setState({ menus: menus.filter( m => m.id !== id)})\n        window.location.href = '/cafe'\n        // return <Redirect to='/cafe' />\n      })\n      .catch( err => {\n        console.log(err);\n      })\n  }\n\n  render() {\n    return(\n      <MenuContext.Provider value={{\n        ...this.state,\n        addMenu: this.addMenu,\n        updateMenu: this.updateMenu,\n        deleteMenu: this.deleteMenu,\n      }}>\n        { this.props.children }\n      </MenuContext.Provider>\n    )\n  }\n}\n\nexport default MenuProvider;","/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/providers/ItemProvider.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/items/ItemForm.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/items/ItemList.js",[],"/Users/doan/Desktop/dpl_winter20/react_on_rails_cafe/client/src/components/items/Item.js",[],["76","77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":3,"column":10,"nodeType":"84","messageId":"85","endLine":3,"endColumn":18},{"ruleId":"78","replacedBy":"86"},{"ruleId":"80","replacedBy":"87"},"no-native-reassign",["88"],"no-negated-in-lhs",["89"],"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar",["88"],["89"],"no-global-assign","no-unsafe-negation"]